{
  "version": 2,
  "name": "jalud-autopflege",
  "builds": [
    {
      "src": "flowedge-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "flowedge-app/dist/flowedge-app/browser"
      }
    },
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/server.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/flowedge-app/dist/flowedge-app/browser/$1"
    }
  ]
}
