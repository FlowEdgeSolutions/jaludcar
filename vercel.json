{
  "buildCommand": "cd flowedge-app && npm install && npm run build:vercel",
  "outputDirectory": "flowedge-app/dist/flowedge-app/browser",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}